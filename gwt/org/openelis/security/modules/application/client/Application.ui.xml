<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:org.openelis.gwt.widget'
  xmlns:t='urn:import:org.openelis.gwt.widget.table'>

  <ui:with type="org.openelis.security.constants.SecurityConstants" field="msg" />
  <ui:with type="org.openelis.gwt.resources.UIResources" field="libRes" />
  <ui:with type="org.openelis.security.modules.main.client.resources.SecurityResources" field="res" />

  <g:HorizontalPanel styleName="{res.style.WhiteContentPanel}">
    <w:CollapsePanel height="635px" styleName="{libRes.collapse.LeftSidePanel}">
      <w:content>
        <g:VerticalPanel>
          <g:VerticalPanel height="10px" />
          <g:HorizontalPanel width="225px">
            <w:AtoZButtons ui:field="atozButtons" multiColumn="false"/>
            <g:VerticalPanel>
              <t:Table ui:field="atozTable" visibleRows="27" styleName="{libRes.table.ScreenTableWithSides}">
                <t:Column label="{msg.name}" width="175">
                  <t:LabelCell />
                </t:Column>
              </t:Table>
              <g:HorizontalPanel horizontalAlignment="CENTER" width="100%">
                <g:HorizontalPanel>
                  <w:Button ui:field="atozPrev" leftIcon="{libRes.buttonPanel.PreviousButtonImage}" enabled="false" />
                  <w:Button ui:field="atozNext" leftIcon="{libRes.buttonPanel.NextButtonImage}" enabled="false" />
                </g:HorizontalPanel>
              </g:HorizontalPanel>
            </g:VerticalPanel>
          </g:HorizontalPanel>
        </g:VerticalPanel>
      </w:content>
    </w:CollapsePanel>
    <g:VerticalPanel>
      <g:AbsolutePanel styleName="{libRes.buttonPanel.ButtonPanelContainer}">
        <g:HorizontalPanel>
          <w:Button css="{libRes.buttonPanel}" ui:field="query" leftIcon="{libRes.buttonPanel.QueryButtonImage}" text="{msg.query}" action="query"
            toggles="true" />
          <w:Button css="{libRes.buttonPanel}" ui:field="previous" leftIcon="{libRes.buttonPanel.PreviousButtonImage}" text="{msg.previous}" action="previous" />
          <w:Button css="{libRes.buttonPanel}" ui:field="next" leftIcon="{libRes.buttonPanel.NextButtonImage}" text="{msg.next}" action="next" />
          <g:AbsolutePanel styleName="{libRes.buttonPanel.ButtonDivider}" />
          <w:Button css="{libRes.buttonPanel}" ui:field="add" leftIcon="{libRes.buttonPanel.AddButtonImage}" text="{msg.add}" action="add" toggles="true" />
          <w:Button css="{libRes.buttonPanel}" ui:field="update" leftIcon="{libRes.buttonPanel.UpdateButtonImage}" text="{msg.update}" toggles="true"
            action="update" />
          <w:Button css="{libRes.buttonPanel}" ui:field="delete" leftIcon="{libRes.buttonPanel.DeleteButtonImage}" text="{msg.delete}" action="delete" />
          <g:AbsolutePanel styleName="{libRes.buttonPanel.ButtonDivider}" />
          <w:Button css="{libRes.buttonPanel}" ui:field="commit" leftIcon="{libRes.buttonPanel.CommitButtonImage}" text="{msg.commit}" action="commit" />
          <w:Button css="{libRes.buttonPanel}" ui:field="abort" leftIcon="{libRes.buttonPanel.AbortButtonImage}" text="{msg.abort}" action="abort" />
        </g:HorizontalPanel>
      </g:AbsolutePanel>
      <g:VerticalPanel>
        <g:HTMLPanel>
          <table class="{res.style.Form}">
            <tr>
              <td class="{res.style.Prompt}">
                <ui:text from="{msg.name}" />
                :
              </td>
              <td>
                <w:TextBox case="LOWER" ui:field="name" maxLength="18" required="true" width="131px" />
              </td>
            </tr>
            <tr>
              <td class="{res.style.Prompt}">
                <ui:text from="{msg.description}" />
                :
              </td>
              <td>
                <w:TextBox ui:field="description" maxLength="60" width="425px" />
              </td>
            </tr>
          </table>
        </g:HTMLPanel>
        <g:VerticalPanel>
          <t:Table horizontalScroll="ALWAYS" ui:field="moduleTable" visibleRows="9" styleName="{libRes.table.ScreenTableWithSides}" verticalScroll="ALWAYS"
            width="600px" header="true">
            <t:Column label="{msg.name}" width="120">
              <t:TextBoxCell>
                <w:TextBox case="LOWER" maxLength="32" required="true" />
              </t:TextBoxCell>
            </t:Column>
            <t:Column label="{msg.description}" width="321">
              <t:TextBoxCell>
                <w:TextBox maxLength="80" />
              </t:TextBoxCell>
            </t:Column>
            <t:Column label="{msg.selectPerm}" width="25">
              <t:CheckBoxCell />
            </t:Column>
            <t:Column label="{msg.addPerm}" width="25">
              <t:CheckBoxCell />
            </t:Column>
            <t:Column label="{msg.updatePerm}" width="25">
              <t:CheckBoxCell />
            </t:Column>
            <t:Column label="{msg.deletePerm}" width="25">
              <t:CheckBoxCell />
            </t:Column>
            <t:Column label="{msg.clause}" width="325">
              <t:TextBoxCell>
                <w:TextBox/>
              </t:TextBoxCell>
            </t:Column>
          </t:Table>
          <g:HorizontalPanel styleName="libRes.table.TableButtonFooter">
            <w:Button ui:field="addModuleButton" leftIcon="{res.style.AddRowButtonImage}" text="{msg.addRow}" />
            <w:Button ui:field="removeModuleButton" leftIcon="{res.style.RemoveRowButtonImage}" text="{msg.removeRow}" />
            <w:Button ui:field="showClauseButton" text="{msg.showClause}" />
          </g:HorizontalPanel>
          <t:Table horizontalScroll="ALWAYS" ui:field="sectionTable" visibleRows="7" styleName="{libRes.table.ScreenTableWithSides}" verticalScroll="ALWAYS"
            width="600px" header="true">
            <t:Column label="{msg.name}" width="120">
              <t:TextBoxCell>
                <w:TextBox case="LOWER" maxLength="20" required="true" />
              </t:TextBoxCell>
            </t:Column>
            <t:Column label="{msg.description}" width="400">
              <t:TextBoxCell>
                <w:TextBox maxLength="60" />
              </t:TextBoxCell>
            </t:Column>
          </t:Table>
          <g:HorizontalPanel>
            <w:Button ui:field="addSectionButton" leftIcon="{res.style.AddRowButtonImage}" text="{msg.addRow}" />
            <w:Button ui:field="removeSectionButton" leftIcon="{res.style.RemoveRowButtonImage}" text="{msg.removeRow}" />
          </g:HorizontalPanel>
        </g:VerticalPanel>
      </g:VerticalPanel>
    </g:VerticalPanel>
  </g:HorizontalPanel>

</ui:UiBinder>