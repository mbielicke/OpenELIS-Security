<ivysettings>
  <settings defaultResolver="nexus-public"/>
  <property name="nexus-public" value="http://ap-serv-j.shl.uiowa.edu:8081/nexus/content/groups/public"/>
  <property name="nexus-snapshots" value="http://ap-serv-j.shl.uiowa.edu:8081/nexus/content/repositories/snapshots"/>
  <resolvers>
    <ibiblio name="nexus-public" m2compatible="true" root="${nexus-public}"/>
    <ibiblio name="nexus-snapshots" m2compatible="true" root="${nexus-snapshots}" checkmodified="true" changingPattern="*-SNAPSHOT"/>
  </resolvers>
</ivysettings>