<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:org.openelis.gwt.widget'
  xmlns:t='urn:import:org.openelis.gwt.widget.table'>

  <ui:with type="org.openelis.security.constants.SecurityConstants" field="msg" />
  <ui:with type="org.openelis.gwt.resources.UIResources" field="libRes" />
  <ui:with type="org.openelis.security.modules.main.client.resources.SecurityResources" field="res" />

  <g:HorizontalPanel  styleName="{res.style.WhiteContentPanel}">
    <w:CollapsePanel height="655px" ui:field="collapsePanel" styleName="{libRes.collapse.LeftSidePanel}">
      <w:content>
        <w:TabPanel height="625px" ui:field="tabPanel" width="280px">
          <w:Tab key="userTab" text="{msg.templates}">
            <g:VerticalPanel>
              <g:HorizontalPanel>
                <w:AtoZButtons ui:field="atozButtons" multiColumn="false"/>
                <g:VerticalPanel>
                  <t:Table horizontalScroll="NEVER" ui:field="atozTable" visibleRows="25" styleName="{libRes.table.ScreenTableWithSides}" verticalScroll="ALWAYS"
                    width="230px" header="true">
                    <t:Column label="{msg.name}" width="210">
                      <t:LabelCell />
                    </t:Column>
                  </t:Table>
                  <g:HorizontalPanel horizontalAlignment="CENTER" width="100%">
                    <g:HorizontalPanel>
                      <w:Button enabled="false" ui:field="atozPrev" leftIcon="{libRes.buttonPanel.PreviousButtonImage}" />
                      <w:Button enabled="false" ui:field="atozNext" leftIcon="{libRes.buttonPanel.NextButtonImage}" />
                    </g:HorizontalPanel>
                  </g:HorizontalPanel>
                </g:VerticalPanel>
              </g:HorizontalPanel>
            </g:VerticalPanel>
          </w:Tab>
          <w:Tab key="moduleTab" text='{msg.modules}'>
            <g:VerticalPanel>
              <g:HTMLPanel>
                <table class="{res.style.Form}">
                  <tr>
                    <td class="{res.style.Prompt}">
                      <ui:text from="{msg.selectApplication}" />:</td>
                    <td>
                      <w:Dropdown field="Integer" ui:field="modAppDropDown" width="125" />
                    </td>
                  </tr>
                </table>
              </g:HTMLPanel>
              <t:Table horizontalScroll="NEVER" ui:field="appModuleTable" visibleRows="24" verticalScroll="ALWAYS" width="257px" header="true" styleName="{libRes.table.ScreenTableWithSides}">
                <t:Column label="{msg.modules}" width="200">
                  <t:LabelCell />
                </t:Column>
              </t:Table>
            </g:VerticalPanel>
          </w:Tab>
          <w:Tab key="sectionTab" text='{msg.sections}'>
            <g:VerticalPanel>
              <g:HTMLPanel>
                <table class="{res.style.Form}">
                  <tr>
                    <td class="{res.style.Prompt}">
                      <ui:text from="{msg.selectApplication}" />:</td>
                    <td>
                      <w:Dropdown field="Integer" ui:field="secAppDropDown" width="125" />
                    </td>
                  </tr>
                </table>
              </g:HTMLPanel>
              <t:Table horizontalScroll="NEVER" ui:field="appSectionTable" visibleRows="24" verticalScroll="ALWAYS" width="257px" header="true" styleName="{libRes.table.ScreenTableWithSides}">
                <t:Column label="{msg.sections}" width="200">
                  <t:LabelCell />
                </t:Column>
              </t:Table>
            </g:VerticalPanel>
          </w:Tab>
        </w:TabPanel>
      </w:content>
    </w:CollapsePanel>
    <!--button panel code -->
    <g:VerticalPanel>
      <g:AbsolutePanel styleName="{libRes.buttonPanel.ButtonPanelContainer}">
        <g:HorizontalPanel>
          <w:Button css="{libRes.buttonPanel}" ui:field="query" leftIcon="{libRes.buttonPanel.QueryButtonImage}" text="{msg.query}" action="query"
            toggles="true" />
          <w:Button css="{libRes.buttonPanel}" ui:field="previous" leftIcon="{libRes.buttonPanel.PreviousButtonImage}" text="{msg.previous}" action="previous" />
          <w:Button css="{libRes.buttonPanel}" ui:field="next" leftIcon="{libRes.buttonPanel.NextButtonImage}" text="{msg.next}" action="next" />
          <g:AbsolutePanel styleName="{libRes.buttonPanel.ButtonDivider}" />
          <w:Button css="{libRes.buttonPanel}" ui:field="add" leftIcon="{libRes.buttonPanel.AddButtonImage}" text="{msg.add}" action="add" toggles="true" />
          <w:Button css="{libRes.buttonPanel}" ui:field="update" leftIcon="{libRes.buttonPanel.UpdateButtonImage}" text="{msg.update}" toggles="true"
            action="update" />
          <w:Button css="{libRes.buttonPanel}" ui:field="delete" leftIcon="{libRes.buttonPanel.DeleteButtonImage}" text="{msg.delete}" action="delete" />
          <g:AbsolutePanel styleName="{libRes.buttonPanel.ButtonDivider}" />
          <w:Button css="{libRes.buttonPanel}" ui:field="commit" leftIcon="{libRes.buttonPanel.CommitButtonImage}" text="{msg.commit}" action="commit" />
          <w:Button css="{libRes.buttonPanel}" ui:field="abort" leftIcon="{libRes.buttonPanel.AbortButtonImage}" text="{msg.abort}" action="abort" />
        </g:HorizontalPanel>
      </g:AbsolutePanel>
      <g:VerticalPanel>
        <g:HTMLPanel>
          <table class="{res.style.Form}">
            <tr>
              <td class="{res.style.Prompt}">
                <ui:text from="{msg.name}" />:</td>
              <td>
                <w:TextBox case="LOWER" ui:field="loginName" maxLength="20" width="145px" />
              </td>
            </tr>
          </table>
        </g:HTMLPanel>
        <t:Table horizontalScroll="ALWAYS" ui:field="userModuleTable" visibleRows="13" styleName="{libRes.table.ScreenTableWithSides}" verticalScroll="ALWAYS"
          header="true">
          <t:Column label="{msg.application}" width="70">
            <t:TextBoxCell>
              <w:TextBox />
            </t:TextBoxCell>
          </t:Column>
          <t:Column label="{msg.name}" width="80">
            <t:TextBoxCell>
              <w:TextBox />
            </t:TextBoxCell>
          </t:Column>
          <t:Column label="{msg.description}" width="200">
            <t:TextBoxCell>
              <w:TextBox />
            </t:TextBoxCell>
          </t:Column>
          <t:Column label="{msg.selectPerm}" width="25">
            <t:CheckBoxCell />
          </t:Column>
          <t:Column label="{msg.addPerm}" width="25">
            <t:CheckBoxCell />
          </t:Column>
          <t:Column label="{msg.updatePerm}" width="25">
            <t:CheckBoxCell />
          </t:Column>
          <t:Column label="{msg.deletePerm}" width="25">
            <t:CheckBoxCell />
          </t:Column>
          <t:Column label="{msg.clause}" width="170">
            <t:TextBoxCell>
              <w:TextBox case="LOWER" />
            </t:TextBoxCell>
          </t:Column>
        </t:Table>
        <g:HorizontalPanel>
          <w:Button ui:field="removeModuleButton" leftIcon="{res.style.RemoveRowButtonImage}" text="{msg.removeRow}" />
          <w:Button ui:field="showClauseButton" text="{msg.showClause}" />
        </g:HorizontalPanel>
        <t:Table horizontalScroll="ALWAYS" ui:field="userSectionTable" visibleRows="7" styleName="{libRes.table.ScreenTableWithSides}" verticalScroll="ALWAYS"
          header="true">
          <t:Column label="{msg.application}" width="70">
            <t:TextBoxCell>
              <w:TextBox />
            </t:TextBoxCell>
          </t:Column>
          <t:Column label="{msg.name}" width="100">
            <t:TextBoxCell>
              <w:TextBox />
            </t:TextBoxCell>
          </t:Column>
          <t:Column label="{msg.description}" width="300">
            <t:TextBoxCell>
              <w:TextBox />
            </t:TextBoxCell>
          </t:Column>
          <t:Column label="{msg.viewPerm}" width="30">
            <t:CheckBoxCell />
          </t:Column>
          <t:Column label="{msg.assignPerm}" width="30">
            <t:CheckBoxCell />
          </t:Column>
          <t:Column label="{msg.completePerm}" width="30">
            <t:CheckBoxCell />
          </t:Column>
          <t:Column label="{msg.releasePerm}" width="30">
            <t:CheckBoxCell />
          </t:Column>
          <t:Column label="{msg.cancelPerm}" width="30">
            <t:CheckBoxCell />
          </t:Column>
        </t:Table>
        <g:HorizontalPanel>
          <w:Button ui:field="removeSectionButton" leftIcon="{res.style.RemoveRowButtonImage}" text="{msg.removeRow}" />
        </g:HorizontalPanel>
      </g:VerticalPanel>
    </g:VerticalPanel>
  </g:HorizontalPanel>
</ui:UiBinder>